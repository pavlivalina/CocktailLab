{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "Email": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "error",
          4
      ],
      "linebreak-style": "off",
      "prefer-destructuring": [
          "error",
          {
              "object": false,
              "array": false
          }
      ],
      "no-param-reassign": 0,
      "import/extensions": [
          "error",
          "always"
      ],
      "no-promise-executor-return": "off"
  }
}